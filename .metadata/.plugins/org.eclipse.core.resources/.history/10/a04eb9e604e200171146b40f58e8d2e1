
public class Solution6 {
	int head;
	int tail;
	int size;
	Integer[] array;
	public Solution6(int cap) {
		array = new Integer[cap];
		head = 0;
		tail = 0;
		size = 0;
	}
	
	public boolean offer(Integer ele) {
		
	}
	
	public Integer peek() {
		return array[head];
	}
	
	public Integer poll() {
		if (size == 0) {
			return null;
		} else {
			size--;
			return array[head++];
		}
		
	}
	
	public int size() {
		return size;
	}
	
	public boolean isEmpty() {
		return size == 0;
	}
}
