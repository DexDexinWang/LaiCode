
public class Solution6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

	  public List<String> subSets(String set) {
		    List<String> res = new ArrayList<String>();
		    if (set == null || set.length() ==0) {
		      return res;
		    }
		    StringBuilder sb = new StringBuilder();
		    subSets(set, sb, 0, res);
		    return res;
		  }
		  
		  public void subSets(String set, StringBuilder sb, int index, List<String> res) {
		    if (index == set.length()) {
		      res.add(sb.toString());
		      return;
		    } 
		    subSets(set, sb.append(set.charAt(index)), index + 1, res);
		    sb.deleteCharAt(sb.length() - 1);
		    while (index + 1 < array.length && array[index] == array[index + 1]) {
		      index++;
		    }
		    subSets(set, sb, index + 1, res);
		  }
}
