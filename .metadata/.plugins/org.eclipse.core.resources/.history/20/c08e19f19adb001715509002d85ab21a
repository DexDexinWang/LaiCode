
public class Solution8 {

	public static void main(String[] args) {

	}
	public static String rightShift(String input, int n) {
		
	}

	private static void reverse (char[] array, int i, int j) {
		while (i < j) {			
			char temp = array[i];
			array[i] = array[j]; 
			array[j] = temp;
			i++;
			j--;
		}
	}
}
