import java.util.List;

public class Solution6 {

	public static void main(String[] args) {
		System.out.println(subSets("aab"));
	}

	  public static List<String> subSets(String set) {
		    List<String> res = new ArrayList<String>();
		    if (set == null || set.length() ==0) {
		      return res;
		    }
		    StringBuilder sb = new StringBuilder();
		    subSets(set, sb, 0, res);
		    return res;
		  }
		  
		  public static void subSets(String set, StringBuilder sb, int index, List<String> res) {
		    if (index == set.length()) {
		      res.add(sb.toString());
		      return;
		    } 
		    subSets(set, sb.append(set.charAt(index)), index + 1, res);
		    sb.deleteCharAt(sb.length() - 1);
		    while (index + 1 < set.length() && set.charAt(index) == set.charAt(index + 1)) {
		      index++;
		    }
		    subSets(set, sb, index + 1, res);
		  }
}
