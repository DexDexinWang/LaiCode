
public class Solution3 {
	public static void main(String[] args) {
		System.out.println(editDistance("abcd", "dcba"));
	}
	
	public static int editDistance(String one, String two) {
		if (one.length() == 0) {
			return two.length();
		} else if (two.length() == 0) {
			return one.length();
		}
	}
}
