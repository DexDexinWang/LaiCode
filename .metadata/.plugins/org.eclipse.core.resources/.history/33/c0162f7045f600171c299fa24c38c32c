import generator.TreeNode;

public class Solution888 {

	public static void main(String[] args) {
		TreeNode root = new TreeNode(10);
		root.left = new TreeNode(5);
		root.right = new TreeNode(15);
		root.left.left = new TreeNode(2);
		root.left.right = new TreeNode(9);
		root.right.left = new TreeNode(11);
		root.right.right = new TreeNode(23);
		root.print();
		System.out.println();
		s(root,5).print();;
	}

	  private static TreeNode s(TreeNode root, int target){ 
		    if (root.key > target && root.left == null ) return root;
		    if (root.key <= target && (root.right == null || root.right.key >= target)) return root;
		    if (root.key <= target) return s(root.right, target);
		    else return s(root.left, target);
		  }

}
