import generator.ListNode;

public class StackByLinkedNode {
	ListNode head;
	int size;
	public StackByLinkedNode() {
		head = null;
		size = 0;
	}
		
		public void push(Integer ele) {
			ListNode newHead = new ListNode(ele);
			newHead.next = head;
			head = newHead;
			size++;
		}
		public Integer peek() {
			if (size == 0) {
				return null;
			}
			return head.key();
		}
		public Integer pop() {
			if (size == 0) {
				return null;
			}
			ListNode temp = head;
			head = head.next;
			temp.next = null;
			size--;
			return temp.key;
		}
		public int size() {
			return size;
		}
		public boolean isEmpty() {
			return size == 0;
		}
}
