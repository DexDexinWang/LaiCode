
public class Solution25 {

	public static void main(String[] args) {

	}
	public int shiftPosition(int[] array) {
		if (array == null || array.length == 0) {
			return 0;
		}
		int low = 0;
		int high = array.length - 1;
		while (low < high) {
			int mid = low + (high - low) / 2;
			if (array[mid] < array[high]) {
				low = mid + 1;
			} else {
				high = mid;
			}
		}
		
		return 0;
	}

}
