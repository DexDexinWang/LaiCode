import java.util.Arrays;

public class Solution1 {

	  public int[] dedup(int[] array) {
		    if (array.length <= 1) {
		      return array;
		    }
		    int slow = 0;
		    int fast = 1; 
		    while (fast < array.length) {
		      if (array[fast] == array[fast - 1]) {
		        fast++;
		        continue;
		      }
		      array[++slow] = array[fast++];
		    }
		    return Arrays.copyOf(array, slow + 1);
		  }
}
