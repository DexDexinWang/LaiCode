	/**
	 * base case: 
	 * dp[0] = array[0]
	 * Induction rule:
	 * dp[i] presents the maximum profit from 0th to ith;
	 * dp[i] = array[i] - minPrice 	where array[i] >= minPrice
	 * dp[i] = array[i] 			otherwise
	 * @param array
	 * @return
	 */
	public int maxProfit(int[] array) {
		if (array.length == 0) {
			return 0;
		}
		int[] dp = new int[array.length];
		dp[0] = array[0];
		int min = array[0];
		int max = dp[0] - min;
		for (int i = 1; i < array.length ; i++) {
			if (array[i] >= minPrice) {
				
			}
		}
	}
}
